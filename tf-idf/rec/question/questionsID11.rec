dt <- data.table(id=1:2, comment=list(list(list(username="michele", date=Sys.time(), message="hello"),
                                           list(username="michele", date=Sys.time(), message="world")),
                                      list(list(username="michele", date=Sys.time(), message="hello"),
                                           list(username="michele", date=Sys.time(), message="world"))))

> dt
   id comment
1:  1  <list>
2:  2  <list>

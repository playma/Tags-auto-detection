    // ... ... ... code ...
    for(int i = 0; i < c.count; ++i) { ... }
} c class Class { ... }

void main() {
    Class c = new Class(); // set to new Class() by default
    // ... ... ... code ...
    for(int i = 0; i < c.count; ++i) { ... }
}

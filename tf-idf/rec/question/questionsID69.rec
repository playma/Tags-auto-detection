                   double left = rect.X >= 20 ? rect.X : 20;
                   double top = rect.Y >= 20 ? rect.Y + 20 : 20;
                   left += this.Padding.Left;
                   top += this.Padding.Top;
                   AssistListBox.SetCurrentValue(ListBox.MarginProperty, new Thickness(left, top, 0, 0));

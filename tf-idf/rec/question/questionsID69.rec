    private String sValue;

    public valueUpdater() {
        this.start();
    }

    public String getValue() {
        return this.sValue;
    }

    public void run() {
        try {
            for (int i = 0; i <= 100; i++) {
                this.sValue = String.valueOf(i);

                try {
                    Thread.sleep(2000);

use 5.014;

my $_ = "14 USD
CHF 15";

while(/(?|
(?<value>[0-9]+)\s*(?<currency>[a-z]+)
|
(?<currency>[a-z]+)\s*(?<value>[0-9]+)
)/igmx) {
    say "Currency: $+{currency}\tValue: $+{value}";
}           

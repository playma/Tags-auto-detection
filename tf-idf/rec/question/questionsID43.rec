        okButton.setEnabled(false);
        nextButton.setEnabled(true);
        break;

    case R.id.next_button:
        nextButton.setEnabled(false);
        okButton.setEnabled(true);
        changeButton.setEnabled(true);
        break;

    case R.id.reset:
        Intent intent = getIntent();
        finish();
        startActivity(intent);
        break;

    default:
        break;
    }

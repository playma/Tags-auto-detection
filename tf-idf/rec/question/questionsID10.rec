    targetCompatibility = 1.6
} compileJava.doFirst {
    println "source compatibility " + sourceCompatibility
} java war jetty gwt war jetty gwt allprojects targetCompatibility sourceCompatibility major version: 50 compileJava.doFirst { println sourceCompatibility } compileJava.sourceCompatibility project.sourceCompatibility tasks.withType(JavaCompile) { sourceCompatibility = "1.6"; targetCompatibility = "1.6" } tasks.withType(JavaCompile) { sourceCompatibility = "1.6"; targetCompatibility = "1.6" } allProjects{}

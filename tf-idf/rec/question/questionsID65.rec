    if (progress > 1.0) {
        progress = 1.0
    }
    let scaleAction = SKAction.scaleXTo(progress, duration: NSTimeInterval(0.3))
    scaleAction.timingMode = .EaseIn
    progressBar.runAction(scaleAction)
}

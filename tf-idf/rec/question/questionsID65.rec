    float  x=350, y=400;


    ////////////

    public GameView(Context context) {
        super(context);
        holder=getHolder();

    }



    @Override
    public void run() {
        while (isItOk){

            if (!holder.getSurface().isValid()){
                continue;
            }
            dest=  new Rect(0, 0, getWidth(), getHeight());
            paint= new Paint();

            c = holder.lockCanvas();
            c.drawARGB(255, 150, 150, 10);
            c.drawBitmap(background, null, dest, paint);
            //c.drawBitmap(yes,30,getHeight()/2,paint);

            //c.drawBitmap(no,getWidth()-200,getHeight()/2,paint);
            paint.setColor(Color.BLUE);
            paint.setTextSize(50);
            paint.setStyle(Paint.Style.FILL);
            paint.setShadowLayer(10f, 10f, 10f, Color.BLACK);
            paint.setTextSize(50);
            paint.setColor(Color.GREEN);
            c.drawText(level, 30, 100, paint);
            c.drawText(credit, 30, 150, paint);
            c.drawText(expiredTime, 30, 200, paint);
            //draw polygon
            paint.setColor(Color.CYAN);
            Path path1 = new Path ();
            path1.moveTo(300, 319);
            path1.lineTo (500, 400);
            path1.lineTo (505, 800);
            path1.lineTo (400, 1000);
            path1.lineTo(200, 800);
            path1.close();
            c.drawPath(path1, paint);

            //animate the ball
            if(pointInPolygon()){
                x+=10-ball.getWidth();
                y+=10-ball.getHeight();


            }
            else if(!pointInPolygon()){
                x+=10+ball.getWidth();
                y+=10+ball.getHeight();

            }
            c.drawBitmap(ball,x,y,null);
            holder.unlockCanvasAndPost(c);


        }

    }
    public void pause(){
        isItOk = false;
        while (true){
            try {
                t.join();
            }catch (InterruptedException e){
                e.printStackTrace();
            }
            break;
        }
        t=null;
    }
    public void resume(){
        isItOk = true;
        t = new Thread(this);
        t.start();
    }


    boolean pointInPolygon() {

        int   i, j=polyCorners-1 ;
        boolean  oddNodes=false;

        for (i=0; i<polyCorners; i++) {
            if ((polyY[i]< y && polyY[j]>=y
                    ||   polyY[j]< y && polyY[i]>=y)
                    &&  (polyX[i]<=x || polyX[j]<=x)) {
                if (polyX[i]+(y-polyY[i])/(polyY[j]-polyY[i])*(polyX[j]-polyX[i])<x) {
                    oddNodes=!oddNodes; }}
            j=i; }

        return oddNodes; }
    public void drawBall(){c.drawBitmap(ball,x,y,paint);}

}


}

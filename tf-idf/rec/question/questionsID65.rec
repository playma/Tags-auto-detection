{
    if (connected) {
        [self sendRegistrationRequestConfirm:NO];
    }
    else{
        [self setMessageLabelString:NSLocalizedString(@"connectionTestFail", nil)];
        [UIView animateWithDuration:0.25 animations:^{
            self.CancelButton.alpha  = 1.0;
        }];
        _blinking = NO;
    }
}

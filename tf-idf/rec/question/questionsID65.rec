        byte[] buffer = null;
        try {
            buffer = Base64.getDecoder().decode(baseStr.getBytes("UTF-8"));
        } catch (IllegalArgumentException e) {
            e.printStackTrace();
            System.exit(1);
        }

        File tmpFile = File.createTempFile("exi", null);
        //tmpFile.deleteOnExit();
        OutputStreamWriter osw = new OutputStreamWriter(new FileOutputStream(tmpFile), "UTF-8");
        for (byte b: buffer)
            osw.write(b);
        osw.close();

        File cmdFile = new File(EFX_CMD);
        if (!cmdFile.isFile() || !cmdFile.canExecute()) {
            System.err.println("WARNING: can not find " + EFX_CMD + "in current dir");
        }
        String cmd = String.format(FULL_CMD, xsdFile, tmpFile);

        System.err.println("xsdFile: " + xsdFile.getAbsolutePath());
        System.err.println("tmpFile: " + tmpFile.getAbsolutePath());
        System.err.println("baseStr length: " + baseStr.length());
        System.err.println("buffer length: " + buffer.length);
        System.err.println("command: " + cmd);

        Process process = Runtime.getRuntime().exec(cmd);
        BufferedReader stdout = new BufferedReader(new InputStreamReader(process.getInputStream()));
        for (String line; (line = stdout.readLine()) != null;)
            System.out.println(line);
        stdout.close();
        BufferedReader stderr = new BufferedReader(new InputStreamReader(process.getErrorStream()));
        for (String line; (line = stderr.readLine()) != null;)
            System.err.println(line);
        stdout.close();
        process.waitFor();
    }

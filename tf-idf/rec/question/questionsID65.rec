{
    [self setMessageLabelString:NSLocalizedString(@"NewDataAvaiableErrorCode", nil)];
    _blinking = NO;
    [UIView animateWithDuration:0.25 animations:^{
        self.CancelButton.alpha  = 1.0;
    }];
}

               Margin="10" TextWrapping="Wrap"
               Text="{Binding Path=Messages}">
        </TextBlock>
    </ScrollViewer> sv.ScrollableHeight - sv.VerticalOffset < 20 < 10 ScrollToEnd() <20 double ScrollToEnd/Bottom() <20 ScrollToBottom

.globl _main
_main:
    movq $_main, %rax
    ret $ as -o test.o test.S 
test.S:3:32-bit absolute addressing is not supported for x86-64
test.S:3:cannot do signed 4 byte relocation $_main $10 $ cat test.S
.globl main
main:
    movq $main, %rax
    ret $ as -o test.o test.S

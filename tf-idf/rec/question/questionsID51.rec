with open("my_c.pik", "wb") as f :
    pickle.dump(c, f, -1) hexdump -C my_c.pik __main__.MyClass MyClass # In Pyramid (different namespace) read the pickled MyClass instance.
with open("my_c.pik", "rb") as f :

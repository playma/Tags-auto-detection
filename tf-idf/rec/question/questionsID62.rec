decade = [4, 3, 6, 6, 5, 5, 5, 7, 6, 6]

puts (1..1000).inject(0) { |sum, n| 
  sum, n = sum + 11, n % 1000 if n > 999
  sum, n = sum + digit[n / 100] + (n % 100 > 0 ? 10 : 7), n % 100 if n > 99
  sum, n = sum + decade[n / 10], n % 10 if n > 19
  sum += digit[n] if n > 0
  sum
}

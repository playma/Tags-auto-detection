void insert(ptr *H, char c) {
    while (*H)
        H = &(*H)->next;

    *H = malloc(sizeof(**H));
    (*H)->data = c;
    (*H)->next = NULL;
}

void freeList(ptr *H) {

    while (*H) {
        ptr tmp = *H;
        *H = (*H)->next;
        free(tmp);
    }
}


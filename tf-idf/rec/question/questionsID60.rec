  admin_password = 'P4ssw0rd'
  domain_index = admin_email.find('@')
  domain = admin_email[domain_index+1:]

  contacts_client = gdata.contacts.client.ContactsClient(domain=domain)
  contacts_client.client_login(email=admin_email,

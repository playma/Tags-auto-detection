            throws HibernateException 
            {
                String id=null;
                try
                {
                    Connection con=arg0.connection();
                    Statement stmt=con.createStatement();
                    ResultSet rs=stmt.executeQuery("select new_sequence.nextval as next from dual");
                    rs.next();
                    int i=rs.getInt("next");
                    if(i<=9)
                        id="c00"+i;
                        else if(i>9 && i<=99)
                            id="c0"+i;
                        else
                            id="c"+i;
                    rs.close();
                    stmt.close();
                    con.close();
                }
                catch(Exception e)

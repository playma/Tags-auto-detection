int temp = var1;
var1 = var2;
var2 = temp; int x = 200;
int y = 100;

x = y - x + (y = x); Apple LLVM version 6.0 (clang-600.0.56) for (int i = -10; i <= 10; i++) {
    for (int j = 10; j >= -10; j--) {
        int x = i, y = j;
        x = y - x + (y = x);

        std::cout << "x = " << x << "\ny = " << y << '\n';
    }
}

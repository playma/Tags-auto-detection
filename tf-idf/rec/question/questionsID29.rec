) FEDERATED ON ( FEDERATION_BY_CUSTOMER = CustomerId ); USE FEDERATION GroupFederation( FEDERATION_BY_CUSTOMER = 1 ) WITH RESET, FILTERING = ON SELECT * FROM Orders INSERT INTO Orders ( OrderId ) VALUES ( 10 );

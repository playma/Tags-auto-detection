if (item != null) { /* Do some stuff */ } MyType
{
  public event MyEventHander MyEvent;
}

...

delegate void MyEventHander ( MyType sender, MyEventArgs e ); public class EventArgs<TContent> : EventArgs
{
  public TContent Content { get; set; }
}

public delegate void EventHandler<TContent>
  ( object sender, EventArgs<TContent> e );

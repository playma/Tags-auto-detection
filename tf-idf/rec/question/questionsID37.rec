		SomeObject so = new SomeObjectBuilder()
		 .WithCalculationMethod(new CalculationMethodBuilder())								                             
                     .WithAmount(amount);

		Mock<IReducer> reducer = new Mock<IReducer>();

		reducer
			.Setup(p => p.Reduce(so, expectedAmount))

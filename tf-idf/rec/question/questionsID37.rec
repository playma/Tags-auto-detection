          return false;
      }

      if (buttonsIdx[pos-1] == -1)   // no button found at that pos
          return false;

      float value = comps[ buttonsIdx[pos-1] ].getPollData();
         // array range is 0-NUM_BUTTONS-1

      return ((value == 0.0f) ? false : true);
    } // end of isButtonPressed()



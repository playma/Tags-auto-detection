    public function ajax( $ajax_type )
    {
        $object_session = new Session();
        $object_session->start();
        switch( $ajax_type ) 
        {
            case 'signin_control': 
                $Object = new ObjectMaker();
                $ObjectSignIn=$Object->makeSignIn();
                $ObjectSignIn->invoke();
                break; 
            case 'signup_control':
                $Object = new ObjectMaker();
                $ObjectSignUp=$Object->makeSignUp();
                $ObjectSignUp->invoke(); 
                break;
            case 'tweet_control':
                $Object = new ObjectMaker();
                $ObjectTweet=$Object->makeTweet();
                $ObjectTweet->add(); 
                break; 
            case 'ControlBookmark_add': 
                $Object = new ObjectMaker();
                $ObjectBookmark = $Object->makeBookmark();
                $ObjectBookmark->add();
                break; 
            case 'ControlBookmark_delete':
                $Object = new ObjectMaker();
                $ObjectBookmark=$Object->makeBookmark();
                $ObjectBookmark->delete(); 
                break; 
            case 'ControlTryIt': 
                $object_session = new Session();
                $object_session->activate( 0, 1, 'Test Account', 'bookmarks') ;
                $message_object = new Message();
                new ControlTryIt( $message_object );
                break; 
            case 'ControlSignOut':
                $object_session = new Session();
                $object_session->finish();
                $message_object = new Message();
                new ControlSignOut( $message_object );
                break;
            default:
                throw new Exception( 'Invalid ajax_type' );
        }
    }
    public function reload()
    {
        ControlReload::reload();
    }
}

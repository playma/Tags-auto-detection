    WRITE(*,*) 'Hello.'
    END

    SUBROUTINE another_message()
    CALL MSG()
    END SUBROUTINE MSG() PROGRAM display SUBROUTINE another_message() PROGRAM SUBROUTINES SUBROUTINE

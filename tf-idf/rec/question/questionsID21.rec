                printf("six\t");
                break;

            case 7:
                printf("seven\t");
                break;

            case 8:
                printf("eight\t");
                break;

            case 9:
                printf("nine\t");
                break;

            default:
                break;
        }

        x = x / 10;

    } while (x != 0);

    return 0;
}

int digitCount (int u)
{
    int cnt = 0;

    do {
        u = u / 10;
        cnt++;

    } while (u != 0);

    return cnt;
}

int reverseNumber (int y)
{
    int cnt, Rev;

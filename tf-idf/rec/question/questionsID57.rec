def handle_undefined_char(self, font, cid):
    if self.debug:
        print >>sys.stderr, 'undefined: %r, %r' % (font, cid)
    return '(cid:%d)' % cid isinstance(u'160', unicode) True False cid = 160
font =  <PDFType1Font: basefont='Helvetica'> def to_unichr(self, cid):
    if self.unicode_map:
        try:
            return self.unicode_map.get_unichr(cid)
        except KeyError:
            pass
    try:
        return self.cid2unicode[cid]
    except KeyError:
        raise PDFUnicodeNotDefined(None, cid) pdf2txt.py -o cidIssue_160_4times.xml cidIssue_160_4times.pdf

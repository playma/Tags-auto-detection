                    || (tm.getSimOperator())
                    .equals(getString(R.string.numeric_att))) {
                // Delete all APNs before adding new APNs

                SettingsDbAdapter.DeleteAPNs("numeric=?",
                        new String[] { getString(R.string.numeric_att) },

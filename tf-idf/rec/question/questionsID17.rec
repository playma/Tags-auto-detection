    movable(movable&&) {}
    movable& operator=(movable&&) { return *this; }
    movable(const movable&) = delete;
    movable& operator=(const movable&) = delete;
}; movable m1(movable(17)); m1 movable m2(m1); m1 std::move movable m2(std::move(m1)); template <typename T>
class container
{
  public:
    explicit container(T&& value) : value_(value) {}
  private:
    T value_;
}; container<movable> c(movable(17)); movable container value std::move explicit container(T&& value) : value_(std::move(value)) {} std::move value movable&& value_(value) movable m1(movable(42)) T&& value

      http://www.springframework.org/schema/integration/spring-integration.xsd
      http://www.springframework.org/schema/beans
      http://www.springframework.org/schema/beans/spring-beans.xsd"> spring-integration.xsd spring-integration-2.1.xsd spring-integration.xsd spring-integration-2.1.xsd

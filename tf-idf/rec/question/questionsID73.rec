             , "UInt", 1)   ; true

   connection := DllCall("libmySQL.dll\mysql_real_connect"
         , "UInt", db
         , "Str", host       ; host name
         , "Str", user       ; user name
         , "Str", password   ; password
         , "Str", database   ; database name
         , "UInt", 3306   ; port
         , "UInt", 0   ; unix_socket
         , "UInt", 0)   ; client_flag

   If (connection = 0)

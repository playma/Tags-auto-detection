    std::vector<uint8_t> result(buffer.size(), 0);
    for (int j = 0; j < buffer.size(); j++)
    {
        uint8_t c = buffer[j];
        if (buffer[j] >= 'a' && buffer[j] <= 'z')
            buffer[j] -= 'a' - 'A';
        uint8_t d = virtualKeyToScanCodeMap[buffer[j]];
        if (c >= 'A' && c <= 'Z')
            d += 0x80;
        for (uint8_t i = 0; i <= 255; i++)
        {
            uint8_t b = (uint8_t)this->key[i * 2];
            if (b == d)
            {
                result[j] = i;
                break;
            }
        }
    }
    return result;
}

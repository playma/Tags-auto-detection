    pt.id = parent_item.typeid 
    and parent_item.id in (
        select
            itemid
        from
            permission
        where
            itemid=parent_item.id  and
            (holder_itemid in (10,100) and level > 0) )
order by
    r.parent_itemid,
    r.relation_typeid,
    r.ordinal

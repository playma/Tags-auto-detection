    a = accumulator()
    a.send(None)
    y = yield 0
    while True:
        y = 2 * a.send(yield y)
 Or, imagining something simpler: def doubler():
    a = accumulator()
    a.send = lambda v: 2 * super(self).send(v)
    return a

        print row, column
        print "from index():",self.index(row, 0, parentIndex).internalPointer().get_name()
        self.emit(QtCore.SIGNAL("dataChanged(QModelIndex,QModelIndex)"), 
                                self.index(row, 0, parentIndex), 
                                self.index(row, 0, parentIndex)) 
        return True 



dput(legend.args[!(legend.args %in% plot.args)]) dput() plot.args <- c(names(formals(plot.default)), names(par())) dput dput(largs.all[(largs.all %in% pargs.all)]) x y pch adj do.call 'plot' plot foo.plot <- function(x,y,...) {

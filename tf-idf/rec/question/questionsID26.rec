    +a12[(x4 ? x4d ) + f4 (x4 ? x4d )];
     a21[(x2 ? x2d ) + f2 (x2 ? x2d )]
    +a22[(x5 ? x5d ) + f5 (x5 ? x5d )];
     a31[(x3 ? x3d ) + f3 (x3 ? x3d )]
    +a32[(x6 ? x6d ) + f6 (x6 ? x6d )]]

